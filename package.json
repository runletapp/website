{
  "name": "runlet-website",
  "description": "Runlet Website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "commit": "git-cz",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,ts}\"",
    "prettier:diff": "prettier --config .prettierrc --list-different \"**/*.{js,ts}\"",
    "prettier-docs": "prettier --config .prettierrc --write \"**/*.md\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-alpha.60",
    "@docusaurus/plugin-ideal-image": "^2.0.0-alpha.39",
    "@docusaurus/preset-classic": "^2.0.0-alpha.59",
    "@docusaurus/theme-search-algolia": "^2.0.0-alpha.61",
    "clsx": "^1.1.1",
    "docusaurus-plugin-sass": "^0.1.9",
    "filesize": "^6.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^16.8.4",
    "react-icons": "^3.10.0",
    "swr": "^0.2.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.10.5",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "babel-loader": "^8.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "prettier": "2.0.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
